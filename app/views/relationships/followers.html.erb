<h2>Follower Users</h2>
<table class='table'>
  <% if @users.exists? %>
  <% @users.each do |user| %>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
      <tr>
        <td><%= image_tag user.get_profile_image(50,50) %></td>
        <td><%= user.name %></td>
        
    <!--フォロー機能実装-->
    <td>フォロー数:<%= user.followings.count %></td>
    <td>フォロワー数:<%= user.followers.count %></td>
    
    <td><%= link_to 'Show', user %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<% else %>
  <p>ユーザーはいません</p>
<% end %>